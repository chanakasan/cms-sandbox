{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "doctrine/mongodb-odm-bundle": "3.0.*@dev",
        "doctrine/mongodb-odm": "1.0.*@dev",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/phpcr-odm": "1.1.0-RC1 as 1.0",
        "doctrine/phpcr-bundle": "1.1.0-beta1 as 1.0",
        "phpcr/phpcr-utils": "1.1.0 as 1.0",
        "jackalope/jackalope-jackrabbit": "1.1.*",
        "sonata-project/doctrine-phpcr-admin-bundle": "1.0.*",
        "symfony-cmf/menu-bundle": "1.0.*",
        "symfony-cmf/block-bundle": "1.0.*",
        "symfony-cmf/routing-bundle": "1.1.*",
        "symfony-cmf/routing-auto-bundle": "1.0.*@alpha",
        "symfony-cmf/tree-browser-bundle": "1.0.*"
    },
    "require-dev": {
        "liip/functional-test-bundle": "1.0.*",
        "phpspec/phpspec": "2.0.*",
        "behat/behat":                       "2.5.*@stable",
        "behat/symfony2-extension":          "*",
        "behat/mink-extension":              "*",
        "behat/mink-browserkit-driver":      "*",
        "behat/mink-goutte-driver":          "*",
        "behat/mink-selenium2-driver":       "*",
        "fzaninotto/faker":                  "1.3.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
